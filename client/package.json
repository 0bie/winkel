{
  "name": "winkel-client",
  "version": "0.0.1",
  "description": "client for winkel project",
  "scripts": {
    "eslint": "./node_modules/.bin/eslint",
    "webpack": "./node_modules/.bin/webpack",
    "prebuild": "rimraf dist",
    "postbuild": "cp -R assets dist/assets",
    "build": "yarn webpack",
    "start": "webpack-dev-server --hot --mode development --env env=dev",
    "build:dev": "yarn build -- --mode development --env env=dev",
    "build:prod": "NODE_ENV=production yarn build -- --mode production --env env=prod",
    "watch:dev": "yarn build:dev --watch",
    "watch:prod": "yarn build:prod --watch",
    "lint": "yarn eslint 'src/**/*.js'"
  },
  "author": "Obie",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.13.0",
    "@babel/core": "7.13.8",
    "@babel/eslint-parser": "7.13.8",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "7.13.8",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-react-jsx": "7.12.17",
    "@babel/preset-env": "7.13.9",
    "autoprefixer": "10.2.4",
    "babel-loader": "8.2.2",
    "compression-webpack-plugin": "7.1.2",
    "css-loader": "5.1.1",
    "dotenv-webpack": "6.0.4",
    "eslint": "7.21.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-react": "7.22.0",
    "html-webpack-inline-source-plugin": "1.0.0-beta.2",
    "html-webpack-plugin": "5.2.0",
    "image-webpack-loader": "7.0.1",
    "mini-css-extract-plugin": "1.3.9",
    "moment-locales-webpack-plugin": "1.2.0",
    "node-sass": "5.0.0",
    "optimize-css-assets-webpack-plugin": "5.0.4",
    "postcss-color-function": "4.1.0",
    "postcss-css-variables": "0.17.0",
    "postcss-import": "14.0.0",
    "postcss-loader": "5.0.0",
    "raw-loader": "4.0.2",
    "rimraf": "3.0.2",
    "sass-loader": "11.0.1",
    "terser-webpack-plugin": "5.1.1",
    "url-loader": "4.1.1",
    "webpack": "5.24.3",
    "webpack-cli": "4.5.0",
    "webpack-dev-server": "4.0.0-beta.0",
    "webpack-manifest-plugin": "3.0.0",
    "webpack-merge": "5.7.3"
  },
  "dependencies": {
    "@0bie/pattern-lib": "0.0.2",
    "@0bie/pattern-lib-react": "1.0.0",
    "axios": "0.21.1",
    "chart.js": "2.9.4",
    "connected-react-router": "5.0.1",
    "core-js": "3.9.1",
    "date-fns": "2.18.0",
    "graphql-tag": "2.11.0",
    "react": "16.8.4",
    "react-chartjs-2": "2.11.1",
    "react-dom": "16.8.4",
    "react-redux": "5.1.1",
    "react-router-dom": "5.2.0",
    "redux": "4.0.1",
    "reselect": "4.0.0"
  }
}
